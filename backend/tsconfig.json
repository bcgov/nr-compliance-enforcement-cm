{
  "compilerOptions": {
    "module": "commonJS",
    "esModuleInterop": true,
    "strict": false,
    "noUncheckedIndexedAccess": true,
    "moduleDetection": "force",
    "resolveJsonModule": true,
    "allowJs": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2022",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "lib": ["es2022"],
    "paths": {
      ".prisma/investigation": ["node_modules/.prisma/investigation/index"],
      ".prisma/shared": ["node_modules/.prisma/shared/index"],
      ".prisma/case_management": ["node_modules/.prisma/case_management/index"]
    }
  },
  "include": ["src/**/*.ts"]
}
